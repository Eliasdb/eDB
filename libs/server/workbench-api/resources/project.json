{
  "name": "workbench-api-resources",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/server/workbench-api/resources/src",
  "projectType": "library",
  "tags": ["scope:server", "context:workbench-api", "type:core"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/libs/server/workbench-api/resources"],
      "options": {
        "command": "tsc -p libs/server/workbench-api/resources/tsconfig.lib.json",
        "color": true
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/server/workbench-api/resources",
        "command": "vitest run --config vitest.config.mts --reporter=verbose"
      }
    }
  }
}
