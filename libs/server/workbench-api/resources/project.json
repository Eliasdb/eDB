{
  "name": "workbench-api-resources",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/server/workbench-api/resources/src",
  "projectType": "library",
  "tags": ["scope:server", "context:workbench-api", "type:core"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/server/workbench-api/resources",
        "main": "libs/server/workbench-api/resources/src/index.ts",
        "tsConfig": "libs/server/workbench-api/resources/tsconfig.lib.json",
        "assets": ["libs/server/workbench-api/resources/*.md"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/server/workbench-api/resources",
        "command": "vitest run --config vitest.config.mts --reporter=verbose"
      }
    }
  }
}
