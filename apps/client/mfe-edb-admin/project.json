{
  "name": "mfe-edb-admin",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/client/mfe-edb-admin/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/client/mfe-edb-admin",
        "main": "apps/client/mfe-edb-admin/src/main.ts",
        "index": "apps/client/mfe-edb-admin/src/index.html",
        "polyfills": [],
        "tsConfig": "apps/client/mfe-edb-admin/tsconfig.app.json",
        "inlineStyleLanguage": "css",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/client/mfe-edb-admin/public"
          }
        ],
        "styles": [
          "apps/client/mfe-edb-admin/src/styles.css",
          "@angular/material/prebuilt-themes/purple-green.css"
        ],
        "customWebpackConfig": {
          "path": "apps/client/mfe-edb-admin/webpack.config.ts"
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "4mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "libs/utils/util-environment/src/lib/environments/environment.ts",
              "with": "libs/utils/util-environment/src/lib/environments/environment.prod.ts"
            }
          ],
          "customWebpackConfig": {
            "path": "apps/client/mfe-edb-admin/webpack.prod.config.ts"
          }
        },
        "staging": {
          "optimization": true,
          "sourceMap": false,
          "outputHashing": "all",
          "namedChunks": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "fileReplacements": [
            {
              "replace": "libs/utils/util-environment/src/lib/environments/environment.ts",
              "with": "libs/utils/util-environment/src/lib/environments/environment.staging.ts"
            }
          ],
          "customWebpackConfig": {
            "path": "apps/client/mfe-edb-admin/webpack.prod.config.ts"
          }
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "libs/utils/util-environment/src/lib/environments/environment.ts",
              "with": "libs/utils/util-environment/src/lib/environments/environment.ts"
            }
          ],
          "customWebpackConfig": {
            "path": "apps/client/mfe-edb-admin/webpack.config.ts"
          }
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "mfe-edb-admin:build:production"
        },
        "development": {
          "buildTarget": "mfe-edb-admin:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4300,
        "publicHost": "http://localhost:4300",
        "hmr": true,
        "liveReload": true,
        "poll": 1000
      },
      "continuous": true
    },
    "serve-mf": {
      "executor": "@nx/angular:module-federation-dev-server",
      "options": {
        "port": 4300,
        "publicHost": "http://localhost:4300",
        "buildTarget": "mfe-edb-admin:build:development",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "mfe-edb-admin:build:development"
        },
        "production": {
          "buildTarget": "mfe-edb-admin:build:production"
        }
      },
      "defaultConfiguration": "development",
      "continuous": true
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "vitestConfig": "apps/client/mfe-edb-admin/vitest.config.ts",
        "passWithNoTests": true
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "mfe-edb-admin:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "mfe-edb-admin:build:development",
        "port": 4300,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "mfe-edb-admin:build:development"
        },
        "production": {
          "buildTarget": "mfe-edb-admin:build:production"
        }
      }
    }
  }
}
