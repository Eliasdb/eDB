      "@eDB-webshop/client-cart": [
        "libs/eDB-webshop/data-access/client-cart/src/index.ts"
      ],
      "@eDB-webshop/feature-book-catalog": [
        "libs/eDB-webshop/features/feature-book-catalog/src/index.ts"
      ],
      "@eDB-webshop/feature-cart": [
        "libs/eDB-webshop/features/feature-cart/src/index.ts"
      ],
      "@eDB-webshop/feature-checkout": [
        "libs/eDB-webshop/features/feature-checkout/src/index.ts"
      ],
      "@eDB-webshop/shared-data": [
        "libs/eDB-webshop/shared/utils/shared-data/src/index.ts"
      ],
      "@eDB-webshop/shared-env": [
        "libs/eDB-webshop/shared/utils/shared-env/src/index.ts"
      ],
