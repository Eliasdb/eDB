        "libs/eDB-webshop/shared/utils/shared-types/src/index.ts"
      ],
      "@eDB-webshop/ui-webshop": [
        "libs/eDB-webshop/shared/ui-webshop/src/index.ts"
      ],
      "@eDB-webshop/util-book-params": [
        "libs/shared/utils/util-book-params/src/index.ts"
      ],
      "@eDB/client-admin": [
        "libs/eDB-admin/data-access/client-admin/src/index"
      ],
      "@eDB/client-auth": ["libs/eDB/data-access/client-auth/src/index"],
      "@eDB/client-catalog": ["libs/eDB/data-access/client-catalog/src/index"],
      "@eDB/client-dashboard": [
        "libs/eDB/data-access/client-dashboard/src/index"
      ],
      "@eDB/client-profile": ["libs/eDB/data-access/client-profile/src/index"],
