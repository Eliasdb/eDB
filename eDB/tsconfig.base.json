      "@eDB/feature-dashboard": [
        "libs/eDB/features/feature-dashboard/src/index"
      ],
      "@eDB/feature-login": ["libs/eDB/features/feature-login/src/index"],
      "@eDB/feature-profile": ["libs/eDB/features/feature-profile/src/index"],
      "@eDB/feature-register": ["libs/eDB/features/feature-register/src/index"],
      "@eDB/feature-shell": ["libs/eDB/features/feature-shell/src/index"],
      "@eDB/shared-env": ["libs/shared/utils/environment/src/index"],
      "@eDB/shared-ui": ["libs/shared/ui/src/index"],
      "@eDB/shared-utils": ["libs/shared/utils/utils/src/index"],
      "@eDB/util-navigation": [
        "libs/shared/utils/util-navigation/src/index.ts"
      ],
      "@eDB/utils-user-params": [
        "libs/shared/utils/utils-user-params/src/index"
