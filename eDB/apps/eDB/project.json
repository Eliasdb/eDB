{
  "name": "eDB",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/eDB/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/eDB",
        "index": "apps/eDB/src/index.html",
        "browser": "apps/eDB/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/eDB/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/eDB/public"
          }
        ],
        "scripts": [],
        "styles": ["apps/eDB/src/app/styles/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["./node_modules"]
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "4mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "libs/config/shared-env/src/lib/environments/environment.ts",

              "with": "libs/config/shared-env/src/lib/environments/environment.prod.ts"
            }
          ]
        },
        "staging": {
          "optimization": true,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "libs/config/shared-env/src/lib/environments/environment.ts",
              "with": "libs/config/shared-env/src/lib/environments/environment.staging.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "libs/config/shared-env/src/lib/environments/environment.ts",
              "with": "libs/config/shared-env/src/lib/environments/environment.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "eDB:build:production"
        },
        "staging": {
          "buildTarget": "eDB:build:staging"
        },
        "development": {
          "buildTarget": "eDB:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "eDB:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "vitestConfig": "apps/eDB/vitest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "eDB:build",
        "port": 4200,
        "staticFilePath": "dist/apps/eDB/browser",
        "spa": true
      }
    }
  }
}
